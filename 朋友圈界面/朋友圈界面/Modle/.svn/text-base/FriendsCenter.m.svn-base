#import "FriendsCenter.h"

@implementation FriendsCenter
+(instancetype)namewithname:(NSString *)name  time:(NSString *)time says:(NSString *)says{
    FriendsCenter *frienscenter=[[FriendsCenter alloc]init];
    frienscenter.name=name;
    frienscenter.time=time;
    frienscenter.says=says;
    return frienscenter;
}
@end
